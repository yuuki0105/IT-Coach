<%= form_with model: [current_user.coach,@yen_per_hour] do |form| %>

  <%= render 'shared/error_messages', model: form.object %>

  <div class="field">
    <%= form.label :price %><br />
    <%= form.number_field :price, autofocus: true %>
  </div>

  <div class="actions">
    <%= form.submit "更新する" %>
  </div>

<% end %>