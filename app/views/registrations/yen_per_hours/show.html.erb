<% content_for :javascripts do %>
  <%= javascript_pack_tag "utilities/form" %>
  <%= javascript_pack_tag "utilities/text" %>
<% end %>

<div class="sm:px-8">

  <%= render "registrations/shared/coach_registration_title", registration_complete_rate: @registration_complete_rate %>

  <h2>時給</h2>

  <%= form_with model: @yen_per_hour, url: registrations_yen_per_hours_path, method: :put do |form| %>

    <%= form.number_field :price %>

    <div class="actions">
      <%= form.submit "保存して次へ" %>
    </div>

  <% end %>

  <%= link_to "戻る", registrations_abilities_path %>

</div>