<%= form_with model: model, url: registrations_careers_path, method: :put, class: "mt-4" do |form| %>

  <%= render 'shared/error_messages', model: form.object %>

  <div id="careers">
    <%= form.fields_for :careers do |career| %>
      <%= render "registrations/careers/career_fields", f: career %>
    <% end %>
    <div>
      <%= link_to_add_association '＋追加する', form, :careers, data: { association_insertion_method: 'before' }, class: "button_add_form" %>
    </div>
  </div>

  <%= render "registrations/shared/buttons", form: form, path: registrations_yen_per_hours_path %>

<% end %>