<%= form_with model: [@coach], url: coach_registrations_abilities_path, method: :post do |form| %>

  <%= render 'shared/error_messages', model: form.object %>

  <%= form.fields_for :abilities do |f| %>

    <div class="field">
      <%= f.label :content %><br />
      <%= f.text_field :content, autofocus: true %>
    </div>

  <% end %>

  <div class="actions">
    <%= form.submit "更新する" %>
  </div>

<% end %>