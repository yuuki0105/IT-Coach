<%= form_with model: [current_user.coach,@ability] do |form| %>

  <%= render 'shared/error_messages', model: form.object %>

  <div class="field">
    <%= form.label :content %><br />
    <%= form.text_field :content, autofocus: true %>
  </div>

  <div class="actions">
    <%= form.submit "更新する" %>
  </div>

<% end %>